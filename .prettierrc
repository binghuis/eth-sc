{
  "plugins": ["prettier-plugin-solidity", "@trivago/prettier-plugin-sort-imports"],
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "printWidth": 200,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "parser": "solidity-parse"
      }
    }
  ]
}
